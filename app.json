{
  "expo": {
    "name": "PitGo",
    "slug": "PitGo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ios.png",
    "scheme": "pitstop",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/ios.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rickabet.PitStop",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#000000",
        "foregroundImage": "./assets/images/ios.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/ios.png",
      "config": {
        "pwa": {
          "display": "standalone",
          "background_color": "#000000",
          "theme_color": "#000000",
          "icon": "./assets/images/ios.png",
          "apple": {
            "mobileWebAppCapable": "yes",
            "touchIcon": "./assets/images/ios.png",
            "barStyle": "black-translucent"
          }
        }
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/ios.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6e058a4d-30eb-49f7-9878-2a869ea39391"
      }
    }
  }
}